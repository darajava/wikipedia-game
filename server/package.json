{
  "name": "server",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/js-levenshtein": "^1.1.1",
    "@types/node": "^16.11.10",
    "@types/ws": "^8.5.3",
    "prettier": "2.7.1",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dom": "^0.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "iconv-lite": "^0.6.3",
    "js-levenshtein": "^1.1.6",
    "mysql": "^2.14.1",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.20",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^2.0.0",
    "typeorm": "0.3.10",
    "types": "file:../types",
    "ws": "^8.11.0"
  },
  "scripts": {
    "build": "rm -r build/*; tsc && cp .env build/",
    "pm2": "npm run build; pm2 start build/index.js --name wiki -f",
    "migration:generate": "npm run build && typeorm migration:generate --dataSource build/data-source.js",
    "migration:run": "npm run build && typeorm migration:run --dataSource build/data-source.js",
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  }
}
